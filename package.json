{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "npm install",
    "start": "npm run build:client && npm run start:server",
    "start:int": "npm run build:client:int && npm run start:server:int",
    "build:client": "webpack -d && babel-node src/setup/server/pwa.js",
    "start:server": "nodemon --exec babel-node -- src/index.js",
    "build:client:int": "webpack -d && babel-node instrumented/setup/server/pwa.js",
    "start:server:int": "nodemon --exec babel-node -- instrumented/index.js",
    "start:prod": "npm run build:client:prod && npm run build:server:prod && npm run start:server:prod",
    "build:client:prod": "webpack -p",
    "build:server:prod": "babel src -s -D -d build",
    "start:server:prod": " node build/index.js",
    "storybook": "start-storybook -p 9000 -c storybook",
    "cy:run": "node_modules/.bin/cypress run",
    "cy:open": "node_modules/.bin/cypress open",
    "report:coverage": "nyc report --reporter=html",
    "delete:reports": "rm -f cypress/results/report/mocha* || true",
    "prereport": "npm run delete:reports",
    "mocha": "./node_modules/.bin/cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "merge": "./node_modules/.bin/mochawesome-merge --reportDir cypress/results/mocha > cypress/results/report/mochawesome.json",
    "generate": "npx mochawesome-report-generator --reportDir cypress/results/report --cdn true --charts true cypress/results/report/mochawesome.json && rm -rf cypress/results/mocha",
    "test": "npm run delete:reports & npm run mocha && npm run merge && npm run generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/node": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@cypress/code-coverage": "^1.12.2",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^4.1.0",
    "dotenv-webpack": "^1.7.0",
    "istanbul-lib-coverage": "^2.0.5",
    "nodemon": "2.0.2",
    "nyc": "^14.1.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "cypress-multi-reporters": "^1.2.1",
    "mocha": "5.2.0",
    "mocha-junit-reporters": "^1.23.6",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.0.1"
  },
  "dependencies": {
    "@babel/runtime": "7.8.3",
    "axios": "0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "5.0.0-alpha.7",
    "gql-query-builder": "3.1.3",
    "js-cookie": "^2.2.1",
    "morgan": "1.9.1",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-jsx": "3.2.4",
    "validator": "12.2.0"
  }
}
